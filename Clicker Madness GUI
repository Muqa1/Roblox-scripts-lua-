ScrollingFrame.Parent = Frame
ScrollingFrame.Active = true
ScrollingFrame.BackgroundColor3 = Color3.fromRGB(166, 166, 166)
ScrollingFrame.Position = UDim2.new(0.268000007, 0, 0.534999847, 0)
ScrollingFrame.Size = UDim2.new(0, 337, 0, 82)

desert.Name = "desert"
desert.Parent = ScrollingFrame
desert.BackgroundColor3 = Color3.fromRGB(109, 109, 109)
desert.Size = UDim2.new(0, 324, 0, 35)
desert.Font = Enum.Font.SourceSans
desert.Text = "desert"
desert.TextColor3 = Color3.fromRGB(0, 0, 0)
desert.TextSize = 14.000
desert.MouseButton1Down:Connect(function()
	function teleportTO(placeCFrame)
		local plr = game.Players.LocalPlayer
		if plr.Character then
			plr.Character.HumanoidRootPart.CFrame = placeCFrame
		end
	end
	teleportTO(game:GetService("Workspace").Worlds.Desert.Teleport.CFrame)
end)

