clickmultiplier.TextSize = 14.000
clickmultiplier.MouseButton1Down:Connect(function()
	do
local args = {
			[1] = "ClickMultiply"
		}

		game:GetService("ReplicatedStorage").Aero.AeroRemoteServices.UpgradeService.BuyUpgrade:FireServer(unpack(args))

	end
end)

